{
  "name": "vue-develop",
  "version": "1.0.0",
  "description": "Vue-develop",
  "author": "Andrii",
  "private": true,
  "scripts": {
    "prerender": "rm -rf prerendered/* && node_modules/webpack/bin/webpack.js --hide-modules --mode none --config=webpack/prerender/webpack.config.js",
    "start": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --hot-styles",
    "start:prod": "NODE_ENV=production webpack-dev-server --host 0.0.0.0 --mode production",
    "start:analyze": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --analyze",
    "build:dev": "rm -rf dist/* && NODE_ENV=development webpack --mode development --env.legacy=true",
    "build": "rm -rf dist/* && NODE_ENV=production webpack --mode production --env.legacy=true",
    "build:analyze": "rm -rf dist/* && NODE_ENV=production webpack --mode production --analyze --profile --json > compilation-stats.json --env.legacy=true",
    "lint": "NODE_ENV=production eslint --ext .js,.vue src",
    "lint:fix": "NODE_ENV=production eslint --ext .js,.vue src test/unit --fix",
    "precommit": "yarn lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^9.0.0",
    "badger-accordion": "^1.2.3",
    "bulma": "^0.7.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.set": "^4.3.2",
    "lodash.startcase": "^4.4.0",
    "lodash.upperfirst": "^4.3.1",
    "luxy.js": "^0.1.0",
    "poststylus": "^1.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "swiper": "^4.5.0",
    "vue": "^2.6.7",
    "vue-affix": "^0.5.2",
    "vue-awesome-swiper": "^3.1.3",
    "vue-bulma-accordion": "^0.4.8",
    "vue-loader": "^15.6.4",
    "vue-router": "^3.0.2",
    "vue-scroll-class": "^1.2.3",
    "vue-scrollactive": "^0.8.0",
    "vue-scrollto": "^2.15.0",
    "vue-slider-component": "^3.0.31",
    "vue-sticky": "^3.3.4",
    "vue-template-compiler": "^2.6.7",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@mdi/font": "^3.9.97",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^4.7.1",
    "fs": "^0.0.1-security",
    "husky": "^0.14.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "prerender-spa-plugin": "^3.4.0",
    "pug-lint-vue": "^0.1.3",
    "sync-request": "^6.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-pug-lint-loader": "^0.0.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-merge": "^4.1.5"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 8"
  ]
}
